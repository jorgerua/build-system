{
  "name": "git-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/git-service",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o ../../dist/libs/git-service ./...",
        "cwd": "libs/git-service"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go test -v -cover ./...",
        "cwd": "libs/git-service"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go vet ./...",
        "cwd": "libs/git-service"
      }
    }
  },
  "tags": ["type:library", "scope:git"]
}
