{
  "name": "image-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/image-service",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build ./...",
        "cwd": "libs/image-service"
      },
      "outputs": ["{workspaceRoot}/dist/libs/image-service"],
      "dependsOn": ["^build"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go test -v -coverprofile=coverage.out ./...",
        "cwd": "libs/image-service"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go vet ./...",
        "cwd": "libs/image-service"
      }
    }
  },
  "tags": ["type:library", "scope:image"],
  "implicitDependencies": ["shared"]
}
